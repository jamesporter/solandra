    "gatsby-source-filesystem": "^4.15.0",
    "gatsby-transformer-remark": "^5.15.0",
    "gatsby-transformer-sharp": "^4.15.",
    "prop-types": "^15.7.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-use-dimensions": "^1.2.1",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
