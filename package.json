{
  "name": "solandra",
  "private": true,
  "description": "A framework for algorithmic art. TypeScript first. Make drawing concepts part of framework. Make APIs for humans.",
  "version": "0.1.0",
  "author": "James Porter <james@amimetic.co.uk>",
  "dependencies": {
    "gatsby": "^2.13.42",
